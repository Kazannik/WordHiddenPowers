      <xs:unique name="StringPowersTableKey" msdata:PrimaryKey="true">
        <xs:selector xpath=".//mstns:TextPowers" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="CategoriesTableKey" msdata:PrimaryKey="true">
        <xs:selector xpath=".//mstns:Categories" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="SubcategoriesTableKey" msdata:PrimaryKey="true">
        <xs:selector xpath=".//mstns:Subcategories" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="DesimalPowersTableKey" msdata:PrimaryKey="true">
        <xs:selector xpath=".//mstns:DecimalPowers" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="ColumnsHeadersTableKey" msdata:PrimaryKey="true">
        <xs:selector xpath=".//mstns:ColumnsHeaders" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="RowsHeadersTableKey" msdata:PrimaryKey="true">
        <xs:selector xpath=".//mstns:RowsHeaders" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="Constraint1">
        <xs:selector xpath=".//mstns:DocumentKeys" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="WordFiles_Constraint1" msdata:ConstraintName="Constraint1">
        <xs:selector xpath=".//mstns:WordFiles" />
        <xs:field xpath="mstns:id" />
      </xs:unique>
      <xs:unique name="Constraint2">
        <xs:selector xpath=".//mstns:WordFiles" />
        <xs:field xpath="mstns:FileName" />
      </xs:unique>
    </xs:element>
    <xs:annotation>
      <xs:appinfo>
        <msdata:Relationship name="Subcategories_Categories" msdata:parent="Subcategories" msdata:child="Categories" msdata:parentkey="category_id" msdata:childkey="id" />
        <msdata:Relationship name="DecimalPowers_Categories" msdata:parent="DecimalPowers" msdata:child="Categories" msdata:parentkey="category_id" msdata:childkey="id" />
        <msdata:Relationship name="DecimalPowers_Subcategories" msdata:parent="DecimalPowers" msdata:child="Subcategories" msdata:parentkey="subcategory_id" msdata:childkey="id" />
        <msdata:Relationship name="DecimalPowers_WordFiles" msdata:parent="DecimalPowers" msdata:child="WordFiles" msdata:parentkey="file_id" msdata:childkey="id" />
        <msdata:Relationship name="TextPowers_Categories" msdata:parent="TextPowers" msdata:child="Categories" msdata:parentkey="category_id" msdata:childkey="id" />
        <msdata:Relationship name="TextPowers_Subcategories" msdata:parent="TextPowers" msdata:child="Subcategories" msdata:parentkey="subcategory_id" msdata:childkey="id" />
        <msdata:Relationship name="TextPowers_WordFiles" msdata:parent="TextPowers" msdata:child="WordFiles" msdata:parentkey="file_id" msdata:childkey="id" />
      </xs:appinfo>
    </xs:annotation>
